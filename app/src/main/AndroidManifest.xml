<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thewhites.myslhs" >

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.android.alarm.permission.SET_ALARM" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <!--
 The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but are recommended.
    -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:background="@drawable/layout_backround"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AboutSLHS"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_about_slhs"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Admissions"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_admissions"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Academics"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_acedemics"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Athletics"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_activities"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".SLHSCalendar"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_calendar"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".LatestNews"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_latest_news"
            android:screenOrientation="portrait" >
        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".MapsActivity"
            android:label="@string/title_activity_maps"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".SplashActivity"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_splash"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".ListActivity"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_list"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Baseball"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_baseball"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".BasketballBoys"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_basketball_boys"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".BasketballGirls"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_basketball_girls"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Cheer"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_cheer"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".CrossCountry"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_cross_country"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Football"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_football"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Golf"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_golf"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".SoccerBoys"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_soccer_boys"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".SoccerGirls"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_soccer_girls"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Softball"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_softball"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".StrengthTraining"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_strength_training"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".TrackField"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_track_field"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Volleyball"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_volleyball"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Wrestling"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_wrestling"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".AppDevelopment"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_app_development"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".TeacherEdition"
            android:background="@drawable/layout_backround"
            android:label="@string/title_activity_teacher_edition"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".TeacherLogin"
            android:label="@string/title_activity_teacher_login" >
        </activity>
        <activity
            android:name=".Faculty"
            android:label="@string/title_activity_image_text_list_view_activity" >
        </activity>
        <activity android:name=".SearchableActivity" >
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>
        <activity
            android:name=".Settings"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name=".Help"
            android:label="@string/title_activity_help" >
        </activity>
        <activity
            android:name=".Seating"
            android:label="@string/title_activity_seating" >
        </activity>
        <activity
            android:name=".Events"
            android:label="@string/title_activity_events"
            android:parentActivityName=".TeacherEdition" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".TeacherEdition" />
        </activity>
        <activity
            android:name=".Lessons"
            android:label="@string/title_activity_lessons" >
        </activity>

        <receiver android:name=".AlertReceiver" />

        <activity
            android:name=".EventsList"
            android:label="@string/title_activity_events_list" >
        </activity>
        <activity
            android:name=".AppDevelopmentTeacher"
            android:label="@string/title_activity_app_development_teacher" >
        </activity>
    </application>

</manifest>
