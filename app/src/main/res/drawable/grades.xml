<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <gradient
                android:endColor="#FFb800"
                android:startColor="#FFCD8E00"
                android:angle="270"/>
            <stroke
                android:width="1dp"
                android:color="#ffb800" />
            <corners
                android:radius="6dp" />
            <padding
                android:left="10dp"
                android:bottom="10dp"
                android:right="10dp"
                android:top="10dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#FFb800"
                android:endColor="#FFCD8E00"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#ffb800" />
            <corners
                android:radius="6dp" />
            <padding
                android:left="10dp"
                android:bottom="10dp"
                android:right="10dp"
                android:top="10dp" />
        </shape>
    </item>
</selector>