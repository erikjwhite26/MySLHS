<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.thewhites.myslhs.Admissions"
    android:background="#FF000000">

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/shorelandLogo"
        android:src="@drawable/shoreland_logo"
        android:background="#ffffffff"
        android:clickable="false"
        android:contentDescription="@string/shoreland_logo"
        android:focusable="true"
        android:longClickable="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/subStatement"
        android:id="@+id/subStatement"
        android:textColor="#ff000000"
        android:background="#ffffb800"
        android:textSize="10pt"
        android:textStyle="bold|italic"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/shorelandLogo"
        android:gravity="center_horizontal" />

    <ViewFlipper
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/viewFlipper"
        android:autoStart="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/subStatement"
        android:flipInterval="2000">

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" >

            <ImageView
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_gravity="center"
                android:adjustViewBounds="true"
                android:scaleType="centerCrop"
                android:src="@drawable/admissions_image_one"
                android:contentDescription="@string/images" />
        </RelativeLayout>
    </ViewFlipper>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="63dp"
        android:id="@+id/linearLayout"
        android:gravity="center"
        android:baselineAligned="false"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="false"
        android:background="#00000000">

        <Button
            android:layout_width="0dp"
            android:layout_height="63dp"
            android:text="@string/bus_routes"
            android:id="@+id/busRoutes"
            android:background="@drawable/menu_buttons"
            style="@style/button_bottoms2"
            android:textAllCaps="false"
            android:layout_weight=".5" />

        <Button
            android:layout_width="0dp"
            android:layout_height="63dp"
            android:text="@string/tuition_and_fees"
            android:id="@+id/tuition"
            android:background="@drawable/menu_buttons"
            style="@style/button_bottoms2"
            android:textAllCaps="false"
            android:singleLine="false"
            android:layout_weight=".5" />

    </LinearLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/busRoutesLayout"
        android:visibility="invisible"
        android:background="#ff000000"
        android:layout_above="@+id/linearLayout"
        android:layout_below="@+id/subStatement" >

        <HorizontalScrollView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/chooseRoute"
            android:visibility="visible" >

            <LinearLayout
