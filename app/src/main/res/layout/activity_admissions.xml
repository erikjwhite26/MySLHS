                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="6:45am"
                            android:id="@+id/textView41"
                            android:textSize="12pt"
                            android:textColor="#ffb800"
                            android:gravity="center" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="                            "
                            android:id="@+id/textView18"
                            android:gravity="center"
                            android:textColor="#ffffffff"
                            android:textSize="12pt"
                            android:singleLine="true" />

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="7:00am"
                            android:id="@+id/textView42"
                            android:textSize="12pt"
                            android:textColor="#ffb800"
                            android:gravity="center" />

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="7:10am"
                            android:id="@+id/textView43"
                            android:textSize="12pt"
                            android:singleLine="false"
                            android:textColor="#ffb800"
                            android:gravity="center" />

                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="240dp"
                        android:layout_height="fill_parent"
                        android:id="@+id/right2"
                        android:gravity="center_horizontal"
                        android:layout_marginRight="10dp" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="                            "
                            android:id="@+id/textView55"
                            android:gravity="center"
                            android:textColor="#ffffffff"
                            android:textSize="12pt"
                            android:singleLine="true" />

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="St. John&apos;s, Howell Ave., Oak Creek"
                            android:id="@+id/textView56"
                            android:textSize="12pt"
                            android:textColor="#ffb800"
                            android:gravity="center" />

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="Zion, South Milwaukee"
                            android:id="@+id/textView57"
                            android:textSize="12pt"
                            android:textColor="#ffb800"
                            android:gravity="center" />

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="St. John&apos;s, Okawood Ave., Oak Creek"
                            android:id="@+id/textView58"
                            android:textSize="12pt"
                            android:textColor="#ffb800"
                            android:gravity="center" />

                    </LinearLayout>
                </LinearLayout>
            </RelativeLayout>
        </ScrollView>

        <ScrollView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:id="@+id/route3"
            android:layout_below="@+id/chooseRoute"
            android:visibility="invisible" >

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_alignParentEnd="false"
                    android:layout_alignParentStart="true"
                    android:id="@+id/route3Layouts"
                    android:gravity="center" >

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="120dp"
                        android:layout_height="fill_parent"
                        android:id="@+id/left3"
                        android:gravity="center_horizontal"
                        android:layout_marginLeft="10dp" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Route 3:"
                            android:id="@+id/textView98"
                            android:gravity="center"
                            android:textColor="#ffffffff"
                            android:singleLine="true"
                            android:textSize="9pt" />

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="6:00am"
                            android:id="@+id/textView99"
                            android:textSize="12pt"
                            android:textColor="#ffb800"
                            android:gravity="center" />

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="6:20am"
                            android:id="@+id/textView100"
                            android:textSize="12pt"
                            android:textColor="#ffb800"
                            android:gravity="center" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="             "
                            android:id="@+id/textView12"
                            android:gravity="center"
                            android:textColor="#ffffffff"
                            android:singleLine="true"
                            android:textSize="12pt" />

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="6:35am"
                            android:id="@+id/textView101"
                            android:textSize="12pt"
                            android:singleLine="false"
                            android:textColor="#ffb800"
                            android:gravity="center" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="             "
                            android:id="@+id/textView17"
                            android:gravity="center"
                            android:textColor="#ffffffff"
                            android:singleLine="true"
                            android:textSize="12pt" />

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="6:50am"
                            android:id="@+id/textView102"
                            android:textSize="12pt"
                            android:singleLine="false"
                            android:textColor="#ffb800"
                            android:gravity="center" />

                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="240dp"
                        android:layout_height="fill_parent"
                        android:id="@+id/right3"
                        android:gravity="center_horizontal"
                        android:layout_marginRight="10dp" >

