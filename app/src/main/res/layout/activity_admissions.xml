                </LinearLayout>
            </RelativeLayout>
        </ScrollView>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/tuitionFeesLayout"
        android:visibility="invisible"
        android:background="#ff000000"
        android:layout_above="@+id/linearLayout"
        android:layout_below="@+id/subStatement">

        <ScrollView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:padding="5dp"
            android:id="@+id/payments">

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Bus Fees"
                    android:id="@+id/busFeesTitle"
                    android:layout_alignParentStart="true"
                    android:textColor="#ffffffff"
                    android:textSize="18pt"
                    android:textStyle="italic"
                    android:layout_marginLeft="2dp"
                    android:layout_marginTop="20dp" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="2dp"
                    android:id="@+id/busFeesUnderline"
                    android:layout_below="@+id/busFeesTitle"
                    android:layout_alignParentStart="true"
                    android:singleLine="true"
                    android:background="#ffffff"
                    android:layout_marginRight="100dp" />

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:id="@+id/busFeesContent"
                    android:layout_below="@+id/busFeesUnderline">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:id="@+id/left"
                        android:layout_marginLeft="5dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/busFees"
                            android:textColor="#ffb800"
                            android:textSize="12pt"
                            android:layout_below="@+id/busFeesUnderline"
                            android:layout_marginTop="10dp"
                            android:text="Bus Fees:" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/space1"
                            android:textColor="#ffb800"
                            android:textSize="12pt"
                            android:layout_below="@+id/busFeesUnderline"
                            android:layout_marginTop="10dp"
                            android:text="         " />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/monthly"
                            android:textColor="#ffb800"
                            android:textSize="12pt"
                            android:layout_below="@+id/busFees"
                            android:layout_marginTop="10dp"
                            android:text="Monthly:" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/space2"
                            android:textColor="#ffb800"
                            android:textSize="12pt"
                            android:layout_below="@+id/busFeesUnderline"
                            android:layout_marginTop="10dp"
                            android:text="         " />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/semester"
                            android:textColor="#ffb800"
                            android:textSize="12pt"
                            android:layout_below="@+id/monthly"
                            android:layout_marginTop="10dp"
                            android:text="Semester:" />
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:id="@+id/right"
                        android:layout_marginRight="5dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/singleOneWay"
                            android:textColor="#ffb800"
                            android:textSize="12pt"
                            android:layout_marginTop="10dp"
                            android:text="One way: $8.00"
                            android:layout_alignTop="@+id/busFees" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/singleSemester"
                            android:textColor="#ffb800"
                            android:textSize="12pt"
                            android:layout_below="@+id/busFeesUnderline"
                            android:layout_marginTop="10dp"
                            android:text="Single Rider: $525" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/singleRoundTrip"
                            android:textColor="#ffb800"
                            android:textSize="12pt"
                            android:layout_below="@+id/busFeesUnderline"
                            android:layout_marginTop="10dp"
                            android:text="Round Trip: $9.75" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/multiMonth"
                            android:textColor="#ffb800"
                            android:textSize="12pt"
                            android:layout_below="@+id/busFeesUnderline"
                            android:layout_marginTop="10dp"
                            android:text="Multi Riders: $160" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/singleMonth"
                            android:textColor="#ffb800"
                            android:textSize="12pt"
                            android:layout_below="@+id/busFeesUnderline"
                            android:layout_marginTop="10dp"
                            android:text="Single Rider: $135" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/multiSemester"
                            android:textColor="#ffb800"
                            android:textSize="12pt"
                            android:layout_below="@+id/busFeesUnderline"
                            android:layout_marginTop="10dp"
                            android:text="Multi Riders: $600" />
                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Lunch Tickets"
                    android:id="@+id/lunchTitle"
                    android:layout_alignParentStart="true"
                    android:textColor="#ffffffff"
                    android:textSize="18pt"
                    android:textStyle="italic"
                    android:layout_marginLeft="2dp"
                    android:layout_marginTop="20dp"
                    android:layout_below="@+id/busFeesContent" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="2dp"
                    android:id="@+id/lunchUnderline"
                    android:layout_below="@+id/lunchTitle"
                    android:layout_alignParentStart="true"
                    android:singleLine="true"
                    android:background="#ffffff"
                    android:layout_marginRight="100dp" />

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:id="@+id/lunch"
                    android:layout_below="@+id/lunchUnderline">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:id="@+id/leftLunch"
                        android:layout_marginLeft="5dp" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/oneDay"
                            android:textColor="#ffb800"
                            android:textSize="12pt"
                            android:layout_below="@+id/busFeesUnderline"
                            android:layout_marginTop="10dp"
                            android:text="One Day Lunch Ticket:" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/fiveDay"
                            android:textColor="#ffb800"
                            android:textSize="12pt"
                            android:layout_below="@+id/oneDay"
                            android:layout_marginTop="10dp"
                            android:text="Five Day Lunch Licket:" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/alacarte"
                            android:textColor="#ffb800"
                            android:textSize="12pt"
                            android:layout_below="@+id/busFeesUnderline"
                            android:layout_marginTop="10dp"
                            android:text="A La Carte Ticket:" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/milk"
                            android:textColor="#ffb800"
                            android:textSize="12pt"
                            android:layout_below="@+id/fiveDay"
                            android:layout_marginTop="10dp"
                            android:text="Ten Punch Milk Ticket:" />
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:id="@+id/rightLunch"
                        android:layout_marginRight="5dp" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/oneDayPrice"
                            android:textColor="#ffb800"
                            android:textSize="12pt"
                            android:layout_marginTop="10dp"
                            android:text="$3.50"
                            android:layout_alignTop="@+id/oneDay" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/fiveDayPrice"
                            android:textColor="#ffb800"
                            android:textSize="12pt"
                            android:layout_below="@+id/busFeesUnderline"
                            android:layout_marginTop="10dp"
                            android:text="$17.00" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/aLaCartePrice"
                            android:textColor="#ffb800"
                            android:textSize="12pt"
                            android:layout_below="@+id/busFeesUnderline"
                            android:layout_marginTop="10dp"
                            android:text="$7.50" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/milkPrice"
                            android:textColor="#ffb800"
                            android:textSize="12pt"
                            android:layout_below="@+id/busFeesUnderline"
                            android:layout_marginTop="10dp"
                            android:text="$1.50" />

                    </LinearLayout>
                </LinearLayout>

            </RelativeLayout>
        </ScrollView>
    </RelativeLayout>
</RelativeLayout>
