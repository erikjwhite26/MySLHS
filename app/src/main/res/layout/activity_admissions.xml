                            android:textSize="12pt" />

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="St. Paul&apos;s, East Troy"
                            android:id="@+id/textView27"
                            android:textSize="12pt"
                            android:textColor="#ffb800"
                            android:gravity="center" />

                        <TextView
                            android:layout_width="fill_parent"
