                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/linearLayout2" >

                <Button
                    style="@style/button_bottoms3"
                    android:layout_width="130dp"
                    android:layout_height="63dp"
                    android:text="Route 1"
                    android:id="@+id/route1Button"
                    android:background="@drawable/menu_buttons"
                    android:textAllCaps="false"
                    android:gravity="center" />

                <Button
                    style="@style/button_bottoms3"
                    android:layout_width="130dp"
                    android:layout_height="63dp"
                    android:text="Route 2"
                    android:id="@+id/route2Button"
                    android:background="@drawable/menu_buttons"
                    android:textAllCaps="false"
                    android:gravity="center" />

                <Button
                    style="@style/button_bottoms3"
                    android:layout_width="130dp"
                    android:layout_height="63dp"
                    android:text="Route 3"
                    android:id="@+id/route3Button"
                    android:background="@drawable/menu_buttons"
                    android:textAllCaps="false"
                    android:gravity="center" />

            </LinearLayout>
        </HorizontalScrollView>

        <ScrollView
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:id="@+id/route1"
            android:layout_below="@+id/chooseRoute"
            android:visibility="visible" >

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" >

